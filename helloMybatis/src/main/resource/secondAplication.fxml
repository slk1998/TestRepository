          <TableColumn fx:id="numberColumn" prefWidth="75.0" text="学号" />
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="姓名" />
            <TableColumn fx:id="groupColumn" prefWidth="75.0" text="组别" />
